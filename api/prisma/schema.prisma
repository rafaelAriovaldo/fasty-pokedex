// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model Pokemon {
  id            Int @id @default(autoincrement())
  name          String
  numberPokedex Int
  img           String
  team  Team[] 
}

model Team {
  id     Int @id @default(autoincrement())
 
  teamName String 
  pokemon Pokemon[] 
  
}